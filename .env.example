# Rate limiting (seconds)
RATE_MIN=0.6
RATE_MAX=1.4

# Browser settings
HEADLESS=true
BROWSER_TIMEOUT=30000

# Scraping limits
MAX_PAGES=10
MAX_RETRIES=3

# User agent
USER_AGENT="Mozilla/5.0 (compatible; CeteseScraper/1.0; +contato@example.com)"

# Paths
DATA_DIR=./data
PDF_DIR=./data/pdfs
CSV_DIR=./data/csv

# Logging
LOG_LEVEL=INFO
LOG_FILE=./data/scraper.log

# PDF parsing
PDF_TIMEOUT=60

# Pipeline
RESUME_ENABLED=true
CHECKPOINT_INTERVAL=100